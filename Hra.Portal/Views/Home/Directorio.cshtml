@model List<Hra.Intra.DA.Usuario>
@{
    Layout = null;
}

<!DOCTYPE html>
<html>
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <link rel='stylesheet' href='~/anexos/css/bootstrap.min.css'>
    <link rel='stylesheet' href='~/anexos/css/dataTables.bootstrap.min.css'>
    <title>Anexos</title>
</head>
<body>
    </br></br>
    <div class="container">
        <div class="row">
            <div class="col-xs-3 col-sm-3 col-md-3"><img src="~/anexos/images/logohra.png" width="100%"></div>
            <div id="titulo" class="col-xs-6 col-sm-6 col-md-6">Directorio Institucional</div>
            <div class="col-xs-3 col-sm-3 col-md-3"><img src="~/anexos/images/logo_gra.png" width="100%"></div>
        </div>



        <table id="example" class="table table-striped table-bordered">
            <thead>
                <tr>
                    <th id="celtitle"><div id="celda"></div></th>
                    <th id="celtitle"><div id="celda">DNI</div></th>
                    <th id="celtitle"><div id="celda">Personal</div></th>
                    <th id="celtitle"><div id="celda">Cargo</div></th>
                    <th id="celtitle"><div id="celda">Correo</div></th>
                </tr>
            </thead>
            <tbody>
                @foreach (var item in Model)
                {
                <tr>
                    <td width="5%"><div id="celda"><img src="~/anexos/images/phone.png" style="width:20px"></div></td>
                    <td width="5%">@item.Dni</td>
                    <td width="30%">@item.Apellidos, @item.Nombres</td>
                    <td width="30%">@item.Cargo</td>
                    <td width="30%" style="text-align:left">@item.Correo</td>
                </tr>
                }

            </tbody>
            <tfoot>

            </tfoot>
        </table>


    </div>

    <script src='~/anexos/js/jquery-3.3.1.js'></script>
    <script src='~/anexos/js/jquery.dataTables.min.js'></script>
    <script src='~/anexos/jsg/dataTables.bootstrap.min.js'></script>

    <script type="text/javascript" class="init">
        $(document).ready(function () {
            $('#example').DataTable();
        });
    </script>
</body>
