@model List<Hra.Intra.DA.Telefono>
@{
    Layout = null;
}

<!DOCTYPE html>
<html>
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <link rel='stylesheet' href='~/anexos/css/bootstrap.min.css'>
    <link rel='stylesheet' href='~/anexos/css/dataTables.bootstrap.min.css'>
    <title>Anexos</title>
</head>
<body>
    </br></br>
    <div class="container">
        <div class="row">
            <div class="col-xs-3 col-sm-3 col-md-3"><img src="~/anexos/images/logohra.png" width="100%"></div>
            <div id="titulo" class="col-xs-6 col-sm-6 col-md-6">LISTA DE ANEXOS</div>
            <div class="col-xs-3 col-sm-3 col-md-3"><img src="~/anexos/images/logo_gra.png" width="100%"></div>

        </div>



        <table id="example" class="table table-striped table-bordered">
            <thead>
                <tr>
                    <th id="celtitle"><div id="celda"></div></th>
                    <th id="celtitle"><div id="celda">Piso</div></th>
                    <th id="celtitle"><div id="celda">Bloque</div></th>
                    <th id="celtitle"><div id="celda">NOMBRE DEL AMBIENTE</div></th>
                    <th id="celtitle"><div id="celda">ANEXO</div></th>
                </tr>
            </thead>
            <tbody>
                @foreach (var item in Model)
                {
                <tr>
                    <td width="5%"><div id="celda"><img src="~/anexos/images/phone.png" style="width:20px"></div></td>
                    <td width="5%">@item.Piso</td>
                    <td width="5%">@item.Bloque</td>
                    <td width="65%">@item.Ambiente</td>
                    <td width="20%"><div id="celda">@item.Anexo</div></td>
                </tr>
                }

            </tbody>
            <tfoot>

            </tfoot>
        </table>


    </div>

    <script src='~/anexos/js/jquery-3.3.1.js'></script>
    <script src='~/anexos/js/jquery.dataTables.min.js'></script>
    <script src='~/anexos/js/dataTables.bootstrap.min.js'></script>

    <script type="text/javascript" class="init">
        $(document).ready(function () {
            $('#example').DataTable();
        });
    </script>
</body>
</html>