
@{
    Layout = null;
}
<!DOCTYPE HTML>
<html>
<head>
    <title>Hospital Regional de Ayacucho</title>
    <link rel="apple-touch-icon" href="~/apple-touch-icon-152x152.png">
    <link rel="shortcut icon" type="image/x-icon" href="~/favicon-32x32.png">
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1, user-scalable=no" />
    
    <link rel="stylesheet" href="~/assets/bootstrap.min.css" integrity="sha384-Vkoo8x4CGsO3+Hhxv8T/Q5PaXtkKtu6ug5TOeNV6gBiFeWPGFN9MuhOf23Q9Ifjh" crossorigin="anonymous">
    <link rel="stylesheet" href="~/assets/css/main1.css" />

    <noscript><link rel="stylesheet" href="~/assets/css/noscript.css" /></noscript>
</head>
<body class="is-preload">
    <div id="wrapper">
        <div id="bg"></div>
        <div id="overlay"></div>
        <div id="main">

            <!-- Header -->
            <header id="header">

                <img src="~/assets/css/images/logo.png">
                <h1>MESA DE SERVICIOS </h1>
                <p>Oficina de Tecnologías de Información</p>
                <nav>
                    <ul>
                        @*<li><a href="@Url.Action("Index","Home")" class="icon solid fa-network-wired" target="_blank"></a></li>*@
                        <li><a href="http://glpi.hrayacucho.gob.pe/glpi/" class="icon solid fa-laptop-code" target="_blank"></a></li>
                        <li><a href="http://repositorio.hrayacucho.gob.pe:8080/share" class="icon solid fa-folder-open" target="_blank"></a></li>
                        <li><a href="https://mail.hrayacucho.gob.pe" class="icon solid fa-envelope" target="_blank"></a></li>
                        <li><a href="https://my.workplace.com/" class="icon solid fa-comments" target="_blank"></a></li>
                        <li><a href="@Url.Action("Anexo","Home")" class="icon solid fa-phone" target="_blank"></a></li>
                    </ul>
                    <ul id="cust">
                        @*<li>Intranet</li>*@
                        <li>Soporte</li>
                        <li>Repositorio</li>
                        <li>Email</li>
                        <li>Chat</li>
                        <li>Anexos</li>
                    </ul>

                </nav>


            </header>



            <!-- Footer -->
            <footer id="footer">
                <span class="copyright">&copy; <a href="#">2020 Oficina de Tecnologías de Información</a> - Hospital Regional de Ayacucho. Todos los Derechos Reservados.</span>
            </footer>

        </div>
    </div>


    <div id="myModal" class="modal fade bd-example-modal-lg" tabindex="-1" role="dialog" aria-labelledby="myLargeModalLabel" aria-hidden="true">
        <div class="modal-dialog modal-lg" role="document">
            <div class="modal-content" style="color:black">

                <div id="carouselExampleIndicators" class="carousel slide" data-ride="carousel">
                    <ol class="carousel-indicators">
                        <li data-target="#carouselExampleIndicators" data-slide-to="0" class="active"></li>
                        <li data-target="#carouselExampleIndicators" data-slide-to="1"></li>
                        @*<li data-target="#carouselExampleIndicators" data-slide-to="2"></li>*@
                    </ol>
                    <div class="carousel-inner">

                        <div class="carousel-item active">
                            <img src="~/img/c6.jpeg" class="d-block w-100 " style="height:410px" alt="...">
                        </div>
                        <div class="carousel-item ">
                            <a href="~/img/c5.jpg" target="_blank">
                                <img src="~/img/c5.jpg" class="d-block w-50 mx-auto " style="height:450px" alt="...">
                            </a>
                        </div>
                        @*
                        <div class="carousel-item">
                            <img src="~/img/c1.jfif" class="d-block w-100" alt="...">
                        </div>*@
                    </div>
                    <a class="carousel-control-prev" href="#carouselExampleIndicators" role="button" data-slide="prev">
                        <span class="carousel-control-prev-icon" aria-hidden="true"></span>
                        <span class="sr-only">Previous</span>
                    </a>
                    <a class="carousel-control-next" href="#carouselExampleIndicators" role="button" data-slide="next">
                        <span class="carousel-control-next-icon" aria-hidden="true"></span>
                        <span class="sr-only">Next</span>
                    </a>
                </div>

            </div>
        </div>
    </div>

    <script src="~/assets/jquery-3.4.1.slim.min.js" integrity="sha384-J6qa4849blE2+poT4WnyKhv5vZF5SrPo0iEjwBvKU7imGFAV0wwj1yYfoRSJoZ+n" crossorigin="anonymous"></script>
   @* <script src="https://cdn.jsdelivr.net/npm/popper.js@1.16.0/dist/umd/popper.min.js" integrity="sha384-Q6E9RHvbIyZFJoft+2mJbHaEWldlvI9IOYy5n3zV9zzTtmI3UksdQRVvoxMfooAo" crossorigin="anonymous"></script>*@

    @*<script src="~/anexos/js/jquery-3.3.1.js"></script>*@
    <script src="~/assets/bootstrap.min.js"></script>
    <script>
        window.onload = function () { document.body.classList.remove('is-preload'); }
        window.ontouchmove = function () { return false; }
        window.onorientationchange = function () { document.body.scrollTop = 0; }
        $(function () {
            $('#myModal').modal('show');
        });
    </script>
</body>
</html>
